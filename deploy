cat cloud.dhall | (echo -n "(" && cat && echo ").main") | dhall-to-text > tmp.tf \
  && cat cloud.dhall | (echo -n "(" && cat && echo ").server") | dhall-to-nix > tmp-in1.nix \
  && cat tmp-in1.nix | sed -e 's/dhallEscapeSlash/"\/"/g' > tmp-in.nix \
  && cat pre.nix tmp-in.nix > tmp.nix \
  && terraform apply -auto-approve
