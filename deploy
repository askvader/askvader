cat cloud.dhall | (echo -n "(" && cat && echo ").main") | dhall-to-text > cloud.tf \
  && cat cloud.dhall | (echo -n "(" && cat && echo ").server") | dhall-to-nix > cloud-in1.nix \
  && cat cloud-in1.nix | sed -e 's/dhallEscapeSlash/"\/"/g' > cloud-in.nix \
  && cat pre.nix cloud-in.nix > cloud.nix \
  && terraform apply -auto-approve
